{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "outDir": "./dist/",
        "sourceMap": true,
        "strict": true,
        "noUnusedLocals": true,
        "module": "es2015",
        "moduleResolution": "node",
        "target": "es6",
        "jsx": "react",
        "noImplicitAny": true,
        "noImplicitThis": true,
        "typeRoots": ["types", "node_modules/@types"]
    },
    "include": ["./Assets/**/js/**/*"]
}
